choice
	prompt "Build which PXP sample"
	default PXP_ROTATE_FLIP

config PXP_ROTATE_FLIP
	bool "PXP rotate & flip sample"

config PXP_SCALER
	bool "PXP scaler sample"

endchoice

source "Kconfig.zephyr"
