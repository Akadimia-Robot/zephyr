config BOARD_QEMU_RISCV64_SMP
	bool
	default y
	select SOC_SERIES_RISCV_VIRT
	select SOC_RISCV_VIRT
	select QEMU_TARGET
	select 64BIT
	select CPU_HAS_FPU_DOUBLE_PRECISION
	select RISCV_ISA_RV64I
	select RISCV_ISA_EXT_ZICSR
	select RISCV_ISA_EXT_ZIFENCEI
