#include "mimxrt1050_evk.dts"

/delete-node/ &s26ks512s0;

/ {
	chosen {
		zephyr,flash = &is25wp064;
	};
};

&flexspi {
	is25wp064: is25wp064@0 {
		compatible = "nxp,imx-flexspi-nor";
		size = <67108864>;
		label = "IS25WP064";
		reg = <0 0x1000>, <0x60000000 DT_SIZE_M(8)>;
		spi-max-frequency = <133000000>;
		status = "okay";
		jedec-id = [9d 70 17];
	};
};
