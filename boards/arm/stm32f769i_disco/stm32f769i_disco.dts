/dts-v1/;
#include <st/stm32f769.dtsi>

/ {
	model = "STMicroelectronics STM32F769I-DISCO board";
	compatible = "st,stm32f769i-disco", "st,stm32f769";

	chosen {
		zephyr,console = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};

	leds {
		compatible = "gpio-leds";
		green_led_1: led@0 {
			gpios = <&gpioj 13 GPIO_INT_ACTIVE_HIGH>;
			label = "User LD1";
		};
		orange_led_2: led@1 {
			gpios = <&gpioj 5 GPIO_INT_ACTIVE_HIGH>;
			label = "User LD2";
		};
/*
		red_led_3: led@2 {
			gpios = <&gpiod 5 GPIO_INT_ACTIVE_HIGH>;
			label = "User LD3";
		};
		blue_led_4: led@3 {
			gpios = <&gpiok 3 GPIO_INT_ACTIVE_HIGH>;
			label = "User LD4";
		};
*/
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		user_button: button@0 {
			label = "User";
			gpios = <&gpioa 0 GPIO_INT_ACTIVE_LOW>;
		};
	};

	aliases {
		led0 = &green_led_1;
		sw0 = &user_button;
	};
};

&usart1 {
	current-speed = <115200>;
	pinctrl-0 = <&usart1_pins_a>;
	pinctrl-names = "default";
	status = "ok";
};

