if SOC_LPC55S69_CPU0

config SOC
	default "lpc55S69_cpu0"

if PINMUX

config PINMUX_MCUX_LPC
	default y

endif # PINMUX

if GPIO

config GPIO_MCUX_LPC
	default y

endif # GPIO

if SERIAL

config UART_MCUX_FLEXCOMM
	default y

endif # SERIAL

if USB

config USB_DC_NXP_IP3511FS
	default y

endif # USB

endif # SOC_LPC55S69_CPU0
