	depends on SOC_STM32H747XX
	select CPU_CORTEX_M7

config BOARD_STM32H747I_DISCO_M4
	def_bool "$(BOARD)" = "stm32h747i_disco_m4"
	depends on SOC_STM32H747XX
	select CPU_CORTEX_M4
