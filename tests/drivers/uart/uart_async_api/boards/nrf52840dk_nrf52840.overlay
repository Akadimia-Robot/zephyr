/ {
	chosen {
		zephyr,console = &uart1;
	};
};

&uart1 {
	current-speed = <115200>;
	status = "okay";
	tx-gpios = <&gpio0 6 0>;
	rx-gpios = <&gpio0 8 0>;
	rts-gpios = <&gpio0 0 0>;
	cts-gpios = <&gpio0 0 0>;
};

&uart0 {
	compatible = "nordic,nrf-uart";
	current-speed = <115200>;
	status = "okay";
	tx-gpios = <&gpio1 1 0>;
	rx-gpios = <&gpio1 2 0>;
	rts-gpios = <&gpio0 5 0>;
	cts-gpios = <&gpio0 7 0>;
};
