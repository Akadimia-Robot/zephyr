menuconfig GPIO_SHAKTI
	bool "shakti GPIO driver"
	depends on DT_HAS_SHAKTI_GPIO0_ENABLED
	depends on SOC_RISCV64_SHAKTI 
	help
	  This option enables the Shakti GPIO driver.

config GPIO_INIT_PRIORITY
	int "GPIO init priority"
	default KERNEL_INIT_PRIORITY_DEFAULT
	help
	  GPIO driver device initialization priority.