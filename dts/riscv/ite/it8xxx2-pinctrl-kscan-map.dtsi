		pinmuxs = <&pinctrlksol 3>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_4_kbs: kscan_kso_4_kbs {
		pinmuxs = <&pinctrlksol 4>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_5_kbs: kscan_kso_5_kbs {
		pinmuxs = <&pinctrlksol 5>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_6_kbs: kscan_kso_6_kbs {
		pinmuxs = <&pinctrlksol 6>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_7_kbs: kscan_kso_7_kbs {
		pinmuxs = <&pinctrlksol 7>;
