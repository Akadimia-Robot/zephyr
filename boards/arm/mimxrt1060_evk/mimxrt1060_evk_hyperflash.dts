#include "mimxrt1060_evk.dts"

/delete-node/ &is25wp064;

/ {
	chosen {
		zephyr,flash = &s26ks512s0;
	};
};

&flexspi {
	s26ks512s0: s26ks512s@0 {
		compatible = "nxp,imx-flexspi-hyperflash";
		size = <DT_SIZE_M(64*8)>;
		label = "S26KS512S";
		reg = <0 0x1000>, <0x60000000 DT_SIZE_M(64)>;
		spi-max-frequency = <166000000>;
		status = "okay";
	};
};
