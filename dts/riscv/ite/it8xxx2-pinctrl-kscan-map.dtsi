		pinmuxs = <&pinctrlksoh 2>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_11_kbs: kscan_kso_11_kbs {
		pinmuxs = <&pinctrlksoh 3>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_12_kbs: kscan_kso_12_kbs {
		pinmuxs = <&pinctrlksoh 4>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_13_kbs: kscan_kso_13_kbs {
		pinmuxs = <&pinctrlksoh 5>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_14_kbs: kscan_kso_14_kbs {
		pinmuxs = <&pinctrlksoh 6>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_15_kbs: kscan_kso_15_kbs {
		pinmuxs = <&pinctrlksoh 7>;
		drive-open-drain;
		bias-pull-up;
	};

	/* KSO[7:0] keyboard scan function */
	kscan_kso_0_kbs: kscan_kso_0_kbs {
		pinmuxs = <&pinctrlksol 0>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_1_kbs: kscan_kso_1_kbs {
		pinmuxs = <&pinctrlksol 1>;
		drive-open-drain;
		bias-pull-up;
	};
	kscan_kso_2_kbs: kscan_kso_2_kbs {
		pinmuxs = <&pinctrlksol 2>;
		drive-open-drain;
