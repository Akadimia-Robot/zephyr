menuconfig CAN_STACKS
	bool "CAN support"
	help
	  This option enables CAN support.

if CAN_STACKS

module = CAN_STACKS
module-str = can_stacks
source "subsys/logging/Kconfig.template.log_config"

endif # CAN_STACKS

