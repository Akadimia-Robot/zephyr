#include "mimxrt1050_evk.dts"

/delete-node/ &hyperflash0;

/ {
	chosen {
		zephyr,flash = &is25wp064;
	};
};

&flexspi {
	is25wp064: is25wp064@60000000 {
		compatible = "issi,is25wp064", "jedec,spi-nor";
		size = <67108864>;
		label = "IS25WP064";
		reg = <0x60000000 DT_SIZE_M(8)>;
		spi-max-frequency = <133000000>;
		status = "okay";
		jedec-id = [9d 70 17];
	};
};
