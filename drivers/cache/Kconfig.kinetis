config CACHE_KINETIS
bool "Kinetis cache driver"
	select CACHE
	select CACHE_HAS_DRIVER
	select CACHE_MANAGEMENT
	help
	  This option enables the CACHE driver for NXP kinetis caches.

choice CACHE_TYPE
	default HAS_EXTERNAL_CACHE if CACHE_KINETIS
endchoice
