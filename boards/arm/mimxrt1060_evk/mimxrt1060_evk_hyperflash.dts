#include "mimxrt1060_evk.dts"

/delete-node/ &is25wp064;

/ {
	chosen {
		zephyr,flash = &hyperflash0;
	};
};

&flexspi {
	hyperflash0: hyperflash@60000000 {
		compatible = "cypress,s26ks512s";
		reg = <0x60000000 DT_SIZE_M(64)>;
		status = "okay";
	};
};
