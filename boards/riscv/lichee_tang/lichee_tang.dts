/dts-v1/;
#include <nuclei_hummingbird.dtsi>

/ {
	model = "Sipeed Lichee Tang";
	compatible = "sipeed,lichee_tang";
	chosen {
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};

&uart0 {
	current-speed = <115200>;
	status = "okay";
};
