&pinctrlkscan {
	/* KSI[7:0] keyboard scan function */
	kscan_ksi_0_kbs: kscan_ksi_0_kbs {
		pinmuxs = <&pinctrlksi 0>;
		bias-pull-up;
	};
	kscan_ksi_1_kbs: kscan_ksi_1_kbs {
		pinmuxs = <&pinctrlksi 1>;
		bias-pull-up;
	};
	kscan_ksi_2_kbs: kscan_ksi_2_kbs {
		pinmuxs = <&pinctrlksi 2>;
		bias-pull-up;
	};
	kscan_ksi_3_kbs: kscan_ksi_3_kbs {
		pinmuxs = <&pinctrlksi 3>;
		bias-pull-up;
	};
	kscan_ksi_4_kbs: kscan_ksi_4_kbs {
		pinmuxs = <&pinctrlksi 4>;
		bias-pull-up;
	};
	kscan_ksi_5_kbs: kscan_ksi_5_kbs {
		pinmuxs = <&pinctrlksi 5>;
		bias-pull-up;
	};
	kscan_ksi_6_kbs: kscan_ksi_6_kbs {
		pinmuxs = <&pinctrlksi 6>;
		bias-pull-up;
	};
	kscan_ksi_7_kbs: kscan_ksi_7_kbs {
		pinmuxs = <&pinctrlksi 7>;
		bias-pull-up;
	};

	/* KSO[15:8] keyboard scan function */
