config SNGCJA5
	bool "SN-GCJA5 laser particle sensor"
	default y
	depends on DT_HAS_PANASONIC_SNGCJA5_ENABLED
	select I2C
	help
	  Enable driver for SN-GCJA5 laser particle sensor.
