config LVL_$(cur-level)_INTR_0$(aggregator)_OFFSET
	int "Level $(prev-level-num) IRQ line for level $(cur-level) aggregator $(aggregator)"
	default 0
	depends on LEVEL_$(cur-level)_INTERRUPTS
	help
	  This is the level $(prev-level-num) interrupt number for level
	  $(cur-level-num) interrupt aggregator $(aggregator).
