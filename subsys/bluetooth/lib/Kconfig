menu "GATT Profiles"

config BT_BAS
	bool
	prompt "Bluetooth BAS support"
	default n
	help
	This option enable the GATT Battery Service.

config BT_CTS
	bool
	prompt "Bluetooth CTS support"
	default n
	help
	This option enable the GATT Current Time Service.

endmenu
