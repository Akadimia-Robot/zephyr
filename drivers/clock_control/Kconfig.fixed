config CLOCK_CONTROL_FIXED_RATE_CLOCK
	bool "Fixed Clock Clock Control"
	depends on DT_HAS_FIXED_CLOCK_ENABLED
	default n
	help
	  Enable driver for device-tree defined fixed clocks.

config CLOCK_CONTROL_FIXED_FACTOR_CLOCK
	bool "Fixed Factor Clock Clock Control"
	default n
	depends on CLOCK_CONTROL_FIXED_RATE_CLOCK
	help
	  Enable driver for device-tree defined fixed factor clocks.
