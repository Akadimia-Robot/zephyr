config BOARD_MIMXRT595_EVK_CM33
	bool
	default y
	select SOC_MIMXRT595S_CM33
	select SOC_SERIES_IMX_RT5XX
	select CODE_DATA_RELOCATION_SRAM
	select SOC_PART_NUMBER_MIMXRT595SFFOC
