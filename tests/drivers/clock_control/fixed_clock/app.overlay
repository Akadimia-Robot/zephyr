/ {
	test {
		test_fixed_rate_clk0: clk {
			compatible = "fixed-clock";
			clock-frequency = <100>;
			#clock-cells = <0>;
		};

		test_fixed_factor_clk0: clkc {
			compatible = "fixed-factor-clock";
			#clock-cells = <0>;
			clock-mult = <0x2>;
			clock-div = <0x3>;
			clocks = <&test_fixed_rate_clk0>;
		};
	};
};
